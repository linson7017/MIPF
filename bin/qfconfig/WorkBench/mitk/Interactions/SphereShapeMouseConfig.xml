<config>
  <event_variant name="Selection" class="MouseMoveEvent" >
  </event_variant>
<!-- Scaling -->
  <event_variant name="InitInteraction" class="MousePressEvent">
      <attribute name="EventButton" value="LeftMouseButton"/>
  </event_variant>
  <event_variant name="ObjectInteraction" class="MouseMoveEvent">
      <attribute name="ButtonState" value="LeftMouseButton"/>
  </event_variant>
  <event_variant name="EndInteraction" class="MouseReleaseEvent">
      <attribute name="EventButton" value="LeftMouseButton"/>
  </event_variant>
</config>
