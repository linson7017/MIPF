<?xml version="1.0" encoding="UTF-8"?>

<Widget id="ImageHoleFilling">
	<VLayout>
		<HLayout>
			<Label text="Image:"/>
			<QmitkDataStorageComboBox id="ImageHoleFilling.ImageSelector"/>
		</HLayout>
		<HLayout>
			<Label text="Radius X"/>
			<LineEdit text="1" id="ImageHoleFilling.RadiusX"/>'
			<Label text="Radius Y"/>
			<LineEdit text="1" id="ImageHoleFilling.RadiusY"/>
			<Label text="Radius Z"/>
			<LineEdit text="1" id="ImageHoleFilling.RadiusZ"/>
		</HLayout>
		<HLayout>
			<Label text="Number Of Iterations"/>
			<LineEdit text="5" id="ImageHoleFilling.NumberOfIterations"/>
		</HLayout>
		<Button text="Fill Holes" msgcommand="ImageHoleFilling.FillHoles"/>
	</VLayout>
</Widget>









